{
  "description": "Removes the Sidebery snapshots from the download history",
  "manifest_version": 2,
  "name": "Remove Sidebery snapshots from downloads",
  "version": "1.0",
  "homepage_url": "https://github.com/DumbIdiot/AutoRemoveSideberySnapshots",
  "icons": {
    "48": "icons/page-48.png"
  },
  "permissions": [
    "downloads",
    "downloads.open"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "type": "module"
  }
}
